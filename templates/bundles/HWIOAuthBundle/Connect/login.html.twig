{% extends '@HWIOAuth/layout.html.twig' %}

{% block body %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in hwi_oauth_resource_owners() %}
        <a href="{{ hwi_oauth_login_url(owner) }}">
            <article>
                <h5>Login with {{ owner | trans({}, 'HWIOAuthBundle') }}</h5>

            </article>
        </a>
    {% endfor %}
{% endblock body %}
